      },
      {
        "id": "Library.extension:directReferenceCode",
        "path": "Library.extension",
        "sliceName": "directReferenceCode",
        "requirements": "A module definition library must describe the direct-reference codes it uses.",
        "min": 0,
        "max": "*",
        "type": [
          {
            "code": "Extension",
